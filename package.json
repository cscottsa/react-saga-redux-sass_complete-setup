{
  "name": "react-universal-saga-modular",
  "description": "Universal & Modular React Kit ft. Redux Saga",
  "author": "Kawi Xiao <kawixiao@gmail.com>",
  "version": "1.2.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/xkawi/react-universal-saga-modular"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xkawi/react-universal-saga-modular/issues"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "modular",
    "modules",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel",
    "saga",
    "redux saga"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "build-dev": "better-npm-run build-dev",
    "start-staging": "better-npm-run start-staging",
    "build-staging": "better-npm-run build-staging",
    "watch-client": "better-npm-run watch-client",
    "watch-client-staging": "better-npm-run watch-client-staging",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "staging": "concurrently --kill-others \"npm run watch-client-staging\" \"npm run start-staging\"",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__SERVER__": false
    },
    "moduleFileExtensions": [
      "",
      "json",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "start-staging": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 8080
      }
    },
    "watch-client-staging": {
      "command": "node webpack/webpack-staging-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 8082
      }
    },
    "build-dev": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-staging": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/staging.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
